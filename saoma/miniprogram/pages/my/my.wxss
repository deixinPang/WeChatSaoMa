.header{
    position: relative;
    height: 160rpx;
    line-height: 100rpx;
    padding:30rpx 30rpx 30rpx 150rpx;
    box-sizing: border-box;
    background: #AB956D;
    font-size: 28rpx;
    color: #fff;
}

.header .thumb{
    position: absolute;
    left: 30rpx;
    top: 30rpx;
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
}

.order-login{
  float: right;
}



.hr {
  width: 100%;
  height: 15px;
  background-color: #f4f5f6;
}

.item {
 position: relative;
    height: 90rpx;
    border-bottom: 1rpx solid #e9e9e9;
    line-height: 90rpx;
    padding-left: 25rpx;

}

navigator {
height: auto;
width: auto;
display: block;
}

.navigator-hover {
  color: #AB956D;
  background: white;
}

.line {
  border: 1px solid #ccc;
  opacity: 0.2;
}
